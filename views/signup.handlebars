<div class="container">
    <div class="card-panel z-depth-3 signup">
        <div class="container">
            <div class="center signup">
                <img
                    alt="logo"
                    src="assets/img/storeLogo.png"
                    width=75
                />
                <h2>Signup</h2>
            </div>
            <div class="center">
                <div class="preloader-wrapper big signup-preloader">
                    <div class="spinner-layer spinner-blue-only">
                        <div class="circle-clipper left">
                            <div class="circle"></div>
                        </div>
                        <div class="gap-patch">
                            <div class="circle"></div>
                        </div>
                        <div class="circle-clipper right">
                            <div class="circle"></div>
                        </div>
                    </div>
                </div>
            </div>
            <form class="signup">
                <div class="input-field">


                    <input
                        type="email"
                        class="form-control validate"
                        id="email-input"
                        required
                    >
                    <label for="email-input">Email</label>

                    <span
                        class="helper-text"
                        data-error="Wrong email format, please try again."
                        data-success="Looks good!"
                    >Enter an email address</span>
                </div>
                <div class="input-field">
                    <label for="first-name-input">First name</label>
                    <input
                        type="text"
                        class="form-control"
                        id="first-name-input"
                        required
                    >
                </div>
                <div class="input-field">
                    <label for="last-name-input">Last name</label>
                    <input
                        type="text"
                        class="form-control"
                        id="last-name-input"
                        required
                    >
                </div>
                <div class="input-field">
                    <label for="address-input">Address</label>
                    <input
                        type="text"
                        class="form-control"
                        id="address-input"
                        name="address-input"
                        autocomplete="off"
                        required
                    >
                </div>
                <div class="input-field">
                    <label for="password-input">Password</label>
                    <input
                        type="password"
                        class="form-control"
                        id="password-input"
                        required
                    >
                </div>
                <div class="input-field">
                    <label for="confirm-password-input">Confirm password</label>
                    <input
                        type="password"
                        class="form-control"
                        id="confirm-password-input"
                        required
                    >
                </div>
                <span
                    hidden="true"
                    class="sign-error center error materialize-red-text"
                >
                    Looks like something went wrong, please try again.
                </span>
                <div class="signup-action center">
                    <button
                        type="submit"
                        class="btn btn-default"
                    >Sign Up</button>
                    <br />
                    <p>Or log in <a href="/login">here</a></p>
                </div>
            </form>
        </div>


        <div class="acknow right">Icons made by <a
                href="https://www.flaticon.com/authors/vitaly-gorbachev"
                title="Vitaly Gorbachev"
            >Vitaly Gorbachev</a> from <a
                href="https://www.flaticon.com/"
                title="Flaticon"
            >www.flaticon.com</a></div>
    </div>
</div>
<script
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDgOH4myK4L5186othubJ2jxoDO3OCww-o&libraries=places&callback=initAutocomplete"
    async
    defer
></script>
<script src="/assets/js/address-autocomplete.js"></script>